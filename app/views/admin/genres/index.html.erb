<h1>ジャンル一覧・追加</h1>
<%= form_with model: Genre.new, url: admin_genres_path do |f| %>
  <h5>ジャンル名</h5>
  <%= f.text_field :name %>
  <%= f.submit '新規登録', class: "btn btn-info" %>
<% end %>

<table></table>
  <tbody>
    <% @genres.each do |genre| %>
      <tr>
        <td><%= genre.name %></td>
        <td><%= link_to "編集する", edit_admin_genre_path(genre.id), class: "btn btn-primary" %></td>
      </tr>
    <% end %>
  </tbody>
</table>